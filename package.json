{
  "name": "custom_voice_commands",
  "version": "1.0.0",
  "description": "Manage voice command to your api's",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"tsoa spec-and-routes\" \"tsoa swagger\" \"tsc --watch\" \"node dist/app.js\"",
    "debug": "concurrently \"tsc -p .\" \"tsoa spec-and-routes\" \"tsc -p .\" \"tsoa swagger\"",
    "postinstall": "tsc -p . && concurrently \"tsoa spec-and-routes\" \"tsoa swagger\"",
    "build": "tsc -p .",
    "tsoa": "tsoa spec-and-routes && tsc",
    "swagger": "tsoa swagger",
    "test": "mocha --check-leaks -r ts-node/register \"test/**/*.spec.ts\" --exit --timeout 10000"
  },
  "engines": {
    "node": "14.x"
  },
  "repository": {
    "private": true
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typegoose/typegoose": "^7.5.0",
    "axios": "^0.21.1",
    "commonjs": "0.0.1",
    "concurrently": "^5.3.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-ws": "^4.0.0",
    "flatted": "^3.1.1",
    "fs": "0.0.1-security",
    "jsonwebtoken": "^8.5.1",
    "microsoft-cognitiveservices-speech-sdk": "^1.15.1",
    "mongoose": "^5.10.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "npm-install-peers": "^1.2.2",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "socket-io": "^1.0.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.5.1",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.1",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.17.0",
    "@types/supertest": "^2.0.10",
    "@types/ws": "^7.4.0",
    "chai": "^4.3.4",
    "mocha": "^8.3.2",
    "mockgoose": "^8.0.4",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
